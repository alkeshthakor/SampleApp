<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <!--
        Base application theme for API 21+. This theme replaces
        MyTheme from resources/values/styles.xml on API 21+ devices.
    -->
  <style name="MyTheme" parent="MyTheme.Base">
    <item name="android:windowAllowEnterTransitionOverlap">true</item>
    <item name="android:windowAllowReturnTransitionOverlap">true</item>
    <item name="android:windowContentTransitions">true</item>
    <item name="android:windowSharedElementEnterTransition">@android:transition/slide_right</item>
    <item name="android:windowSharedElementExitTransition">@android:transition/slide_left</item>
    <item name="android:navigationBarColor">@color/Material_Teal_500</item>
  
  </style>
  
  
  <style name="myButtonThemeMaterial" parent="riasedbuttonthem">
    <item name="android:elevation">1dp</item>
    <item name="android:translationZ">1dp</item>
  </style>
      
  <style name="myButtonTheme" parent="myButtonThemeMaterial">
    <item name="android:background">@drawable/raised_button_background</item>
 </style>
 
  <style name="myButtonThemeRight" parent="myButtonThemeMaterial">
    <item name="android:background">@drawable/booth_left_button_background</item>
 </style>
 


  
